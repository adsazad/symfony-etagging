{
    "name": "adsazad/symfony-etagging",
    "description": "etagging bundle for symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "~4.4|5.0.*",
        "symfony/yaml": "~4.4|5.0.*"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Adsazad\\EtaggingBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adsazad\\EtaggingBundle\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
